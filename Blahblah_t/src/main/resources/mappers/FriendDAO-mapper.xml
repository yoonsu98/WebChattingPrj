<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mabatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mappers.FriendDAO-mapper">

	<select id="getFollowingMemberInfo" parameterType="String" resultType="com.gaon.prj.member.vo.MemberVO">
	select id,nickname from member_tbl where id in (select fiid from friend_tbl where feid=#{id})
	</select>
</mapper>